{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "source": "arch_small",
        "padding": {
            "top": 1
        },
        "color": {
            "1": "94",
            "2": "34"
        }
    },
    "display": {
        "separator": "  "
    },
    "modules": [
        "break",
        {
            "type": "title",
            "color": {
                "user": "94",
                "at": "37",
                "host": "94"
            }
        },
        {
            "type": "os",
            "key": "os    ",
            "keyColor": "34"
        },
        {
            "type": "kernel",
            "key": "kernel",
            "keyColor": "34"
        },
        {
            "type": "host",
            "format": "{5} {1}",
            "key": "host  ",
            "keyColor": "34"
        },
        {
            "type": "packages",
            "format": "{} (pacman)",
            "key": "pkgs  ",
            "keyColor": "34"
        },
        {
            "type": "uptime",
            "format": "{2}h {3}m",
            "key": "uptime",
            "keyColor": "34"
        },
        {
            "type": "memory",
            "key": "memory",
            "keyColor": "34"
        },
        "break"
    ]
}
